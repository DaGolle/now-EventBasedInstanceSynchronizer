        <api_name>x_snc_ebis.OutboundEventUtil</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Process outbound EBIS events</description>
        <name>OutboundEventUtil</name>
        <script><![CDATA[class OutboundEventUtil {
	constructor() {
	}

	process(eventName, table, payload) {
		// 'out' here becomes 'in' for the target
		const inboundEventName = eventName.replace('.out.', '.in.');
		gs.info(`Processing outbound event ${inboundEventName} ${table} ${payload}`);
	}
}
]]></script>
